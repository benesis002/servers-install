[{
		"type": "container",
		"title": "Jdownloader",
		"description": "Deploy Jdownloader Container",
		"categories": [
			"docker"
		],
		"platform": "linux",
		"logo": "http://zen-info.fr/wp-content/uploads/2011/03/jdownloader.png",
		"image": "plusminus/jdownloader2-headless:latest",
		"network": "bridge",
		"restart_policy": "always",
		"volumes": [
			"/opt/JDownloader/cfg",
			"/downloads"
		]
	},
	{
		"type": "container",
		"title": "Plex Media Server",
		"description": "Deploy a PlexMediaServer Container",
		"categories": [
			"docker"
		],
		"platform": "linux",
		"logo": "http://ift.tt/1YIpkvj",
		"image": "plexinc/pms-docker",
		"network": "bridge",
		"restart_policy": "always",
		"volumes": [
			"/config",
			"/transcode",
			"/data"
		],
		"ports": [
			"32400/tcp",
			"3005/tcp",
			"8324/tcp",
			"32469/tcp",
			"1900/udp",
			"32410/udp",
			"32412/udp",
			"32413/udp",
			"32414/udp"
		],
		"env": [{
				"name": "TZ",
				"label": "TimeZone",
				"set": "Europe/Paris"
			},
			{
				"name": "PLEX_CLAIM",
				"label": "Plex claimToken"
			},
			{
				"name": "ADVERTISE_IP",
				"label": "Host Server IP",
				"description": "http://<hostIPAddress>:32400/"
			}
		]
	}
]
