[
  {
    "type": "container",
    "title": "Jdownloader",
    "description": "Deploy Jdownloader Container",
    "categories": ["docker"],
    "platform": "linux",
    "logo": "http://zen-info.fr/wp-content/uploads/2011/03/jdownloader.png",
    "image": "plusminus/jdownloader2-headless:latest",
    "network": "bridge",
    "restart_policy":"always",
    "volumes": ["/opt/JDownloader/cfg","/downloads"],
    "command":"echo "{"password":"$MYJDOWNLOADER_PASSWORD","email":"$MYJDOWNLOADER_USER"}" >> /opt/JDownloader/cfg/org.jdownloader.api.myjdownloader.MyJDownloaderSettings.json && /opt/JDownloader/startJD2.sh",
    "env": [{
      "name": "MYJDOWNLOADER_USER",
      "description": "Username of your myjdownloader account"
    },{
      "name": "MYJDOWNLOADER_PASSWORD",
      "description": "Password of your myjdownloader account"
    }]
  }
]
