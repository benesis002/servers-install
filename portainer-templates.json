[
  {
    "type": "container",
    "title": "Jdownloader",
    "description": "Deploy Jdownloader Container",
    "categories": [
      "docker"
    ],
    "platform": "linux",
    "logo": "http://zen-info.fr/wp-content/uploads/2011/03/jdownloader.png",
    "image": "plusminus/jdownloader2-headless:latest",
    "network": "bridge",
    "restart_policy": "always",
    "volumes": [
      "/opt/JDownloader/cfg",
      "/downloads"
    ]
  },
  {
    "type": "container",
    "title": "Plex Media Server",
    "description": "Deploy a PlexMediaServer Container",
    "categories": [
      "docker"
    ],
    "platform": "linux",
    "logo": "http://ift.tt/1YIpkvj",
    "image": "plexinc/pms-docker:latest",
    "network": "bridge",
    "restart_policy": "always",
    "volumes": [
      "/config",
      "/transcode",
      "/data"
    ],
    "ports": [
      "32400/tcp",
      "3005/tcp",
      "8324/tcp",
      "32469/tcp",
      "1900/udp",
      "32410/udp",
      "32412/udp",
      "32413/udp",
      "32414/udp"
    ],
    "env": [
      {
        "name": "TZ",
        "label": "TimeZone",
        "set": "Europe/Paris"
      },
      {
        "name": "PLEX_CLAIM",
        "label": "Plex claimToken"
      },
      {
        "name": "ADVERTISE_IP",
        "label": "Host Server IP",
        "description": "http://<hostIPAddress>:32400/"
      }
    ]
  },
  {
    "type": "container",
    "title": "Watchtower",
    "description": "Deploy Watchtower Container",
    "categories": [
      "docker"
    ],
    "platform": "linux",
    "logo": "https://freemobileapk.com/wp-content/uploads/2014/09/com_jvillella_watchtowerlibrary.png",
    "image": "v2tec/watchtower:latest",
    "network": "bridge",
    "restart_policy": "always",
    "volumes": [
      {
        "container": "/var/run/docker.sock",
        "bind": "/var/run/docker.sock"
      }
    ],
    "command": "--schedule '0 3 0 * * *' --cleanup --debug"
  },
  {
    "type": "container",
    "title": "Watchtower ARM",
    "description": "Deploy Watchtower Container",
    "categories": [
      "docker"
    ],
    "platform": "linux",
    "logo": "https://freemobileapk.com/wp-content/uploads/2014/09/com_jvillella_watchtowerlibrary.png",
    "image": "v2tec/watchtower:armhf-latest",
    "network": "bridge",
    "restart_policy": "always",
    "volumes": [
      {
        "container": "/var/run/docker.sock",
        "bind": "/var/run/docker.sock"
      }
    ],
    "command": "--schedule '0 3 0 * * *' --cleanup --debug"
  },
  {
    "type": "container",
    "title": "Jeedom Server",
    "description": "Deploy a Jeedom Container",
    "categories": [
      "domotique"
    ],
    "platform": "linux",
    "logo": "https://www.jeedom.com/site/logo.png",
    "image": "jeedom/jeedom:latest",
    "network": "host",
    "restart_policy": "always",
    "volumes": [
      "/var/www/html",
      "/mnt/backup"
    ],
    "env": [
      {
        "name": "ROOT_PASSWORD",
        "label": "ROOT_PASSWORD"
      }
    ]
  },
  {
    "type": "container",
    "title": "MySQL DB",
    "description": "Deploy a MySQL Database Container",
    "categories": [
      "database"
    ],
    "platform": "linux",
    "logo": "https://upload.wikimedia.org/wikipedia/en/thumb/6/62/MySQL.svg/1200px-MySQL.svg.png",
    "image": "mysql:latest",
    "network": "dedicated",
    "restart_policy": "always",
    "volumes": [
      "/var/lib/mysql"
    ],
    "env": [
      {
        "name": "MYSQL_ROOT_PASSWORD",
        "label": "MYSQL_ROOT_PASSWORD"
      },
      {
        "name": "MYSQL_DATABASE",
        "label": "MYSQL_DATABASE"
      },
      {
        "name": "MYSQL_USER",
        "label": "MYSQL_USER"
      },
      {
        "name": "MYSQL_PASSWORD",
        "label": "MYSQL_PASSWORD"
      }
    ]
  },
  {
    "type": "container",
    "title": "Duplicati",
    "description": "Deploy Duplicati Container",
    "categories": [
      "docker"
    ],
    "platform": "linux",
    "logo": "https://www.duplicati.com/images/duplicati-fb-share-v1.png",
    "image": "linuxserver/duplicati:latest",
    "network": "bridge",
    "restart_policy": "always",
    "ports": [
      "8200/tcp"
    ],
    "volumes": [
      {
        "container": "/source",
        "bind": "/var/lib/docker/volumes"
      },
      "/config"
    ],
    "env": [
      {
        "name": "PGID",
        "label": "PGID",
        "set": "0"
      },
      {
        "name": "PUID",
        "label": "PUID",
        "set": "0"
      }
    ]
  },
  {
    "type": "container",
    "title": "Duplicati ARM",
    "description": "Deploy Duplicati Container",
    "categories": [
      "docker"
    ],
    "platform": "linux",
    "logo": "https://www.duplicati.com/images/duplicati-fb-share-v1.png",
    "image": "lsioarmhf/duplicati:latest",
    "network": "bridge",
    "restart_policy": "always",
    "ports": [
      "8200/tcp"
    ],
    "volumes": [
      {
        "container": "/source",
        "bind": "/var/lib/docker/volumes"
      },
      "/config"
    ],
    "env": [
      {
        "name": "PGID",
        "label": "PGID",
        "set": "0"
      },
      {
        "name": "PUID",
        "label": "PUID",
        "set": "0"
      }
    ]
  },
  {
    "type": "stack",
    "title": "Jeedom",
    "description": "Deploy Jeedom server",
    "note": "Deploys a Jeedom with a MySQL database",
    "categories": [
      "domotique"
    ],
    "platform": "linux",
    "logo": "https://www.jeedom.com/site/logo.png",
    "volumes": [
      {
        "container": "/zwave_usb_key",
        "bind": "/dev/bus/usb/001/004"
      },
      "/db_data",
      "/jeedom_data",
      "/jeedom_backup"
    ],
    "repository": {
      "url": "https://github.com/benesis002/servers-install",
      "stackfile": "stack-jeedom.yml"
    },
    "env": [
      {
        "name": "MYSQL_ROOT_PASSWORD",
        "label": "Database root password",
        "description": "Password used by the MySQL root user."
      },
      {
        "name": "JEEDOM_ROOT_PASSWORD",
        "label": "Jeedom Server root password",
        "description": "Password used by Jeedom root user."
      }
    ]
  }
]
